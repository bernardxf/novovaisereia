*
	margin 0
	outline none
	padding 0
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
a
	text-decoration none
ul
	list-style none
body
	font-family 'ralewayregular'
	font-size 14px

h2
	color #0A5299
	display block
	font-family 'ralewaybold'
	font-size 40px
	text-align center
	text-transform uppercase

h3
	color #535353
	display block
	font-size 23px
	font-family 'ralewaylight'
	margin 0 0 30px
	text-align center

.button
	border 2px solid #00529B
	color #005299
	display block
	font-size 20px
	height 55px
	line-height 55px
	margin 15px auto 0
	text-align center
	text-transform uppercase
	width 275px
	&:hover
		background-color #00529B
		color #fff

header
section
footer
	display inline-block
	position relative
	vertical-align top
	width 100%

section
	padding 40px 0

header
	background-color #125697
	.content
		height 100px
		padding 0
	#menu-mobile
		cursor pointer
		display none
		float right
		height 25px
		margin 20px 0 0
		position relative
		transform rotate(0deg)
		transition .5s ease-in-out
		width 45px
		span
			background #fff
			display block
			height 5px
			border-radius 5px
			left 0
			opacity 1
			position absolute
			transform rotate(0deg)
			transition .25s ease-in-out
			width 100%
			&:nth-child(1)
				top 0px
			&:nth-child(2)
			&:nth-child(3)
				top 10px
			&:nth-child(4)
				top 20px
		&.ativo
			span
				&:nth-child(1)
					top 10px
					width 0%
					left 50%
				&:nth-child(2)
					transform rotate(45deg)
				&:nth-child(3)
					transform rotate(-45deg)
				&:nth-child(4)
					left 50%
					top 10px
					width 0%
	.social
		display block
		float right
		margin 15px 0 0
		a
			color #fff
			display inline-block
			margin 0 12px
			position relative
			vertical-align top
			&:before
				display block
				font-family icomoon
				font-size 20px
			&.inst:before
				content '\e902'
			&.ytube:before
				content '\e906'
			&.fbook:before
				content '\e901'
	.logo
		background-image url(../images/logo.png)
		background-position 0 0
		background-repeat no-repeat
		background-size cover
		display block
		height 61px
		float left
		margin 20px 0
		width 237px
	ul
		clear both
		display block
		float right
		margin -40px 0 0
		li
			display inline-block
			margin 0 12px
			vertical-align top
			a
				color #fff
				display block
				font-size 17px
				text-transform uppercase
				&:hover
					text-decoration underline

#slideshow
	display block
	position relative
	height auto
	padding 0
	width 100%
	.col-md-12
		padding 0
	.content-slide
		display block
		height 650px
		overflow hidden
		width 100%
		.img1
		.img2
		.img3
		.img4
		.img5
			background-position center
			background-repeat no-repeat
			background-size cover
			display block
			float left
			height 650px
			position relative
			width 100%;
		.img1
			background-image url(../images/slide/01.png)
		.img2
			background-image url(../images/slide/02.png)
		.img3
			background-image url(../images/slide/03.png)
		.img4
			background-image url(../images/slide/04.png)
		.img5
			background-image url(../images/slide/05.png)

#competicao
#contato
	background-color #F5F6F7

#sobre
	img
		display block
		width 100%
	p
		color #535353
		display block
		font-family 'ralewaylight'
		font-size 22px
		margin 0 0 15px
	.patrocinadores
		display block
		margin 30px 0 0
		text-align center
		width 100%
		h4
			color #535353
			display block
			font-size 16px
			margin 0 auto 20px
			text-transform uppercase
		.patrocinios
			display block
			margin 0 auto 30px
			a
				display inline-block
				font-size 0
				height 80px
				margin 0 20px
				vertical-align top
				width 80px
				&.crossfit
					background url(../images/patrocinios/crossfit.png) 0 0 no-repeat
				&.nutriu
					background url(../images/patrocinios/nutriu.jpg) 0 0 no-repeat
				&.mundial
					background url(../images/patrocinios/mundial.png) 0 0 no-repeat
				&.soul
					background url(../images/patrocinios/soul.png) 0 0 no-repeat

#competicao
	ul
		display block
		overflow hidden
		li
			display inline-block
			margin 0 20px 20px
			vertical-align top
			width 355px
			.foto
				background-position center
				background-repeat no-repeat
				background-size cover
				display block
				height 200px
				width 100%
				&.festival5
					background-image url(../images/fotos/5festival.jpg)
				&.festival4
					background-image url(../images/fotos/4festival.jpg)
				&.mineiro-inverno
					background-image url(../images/fotos/mineiro-inverno.jpg)
				&.festival3
					background-image url(../images/fotos/3festival.jpg)
				&.festival2
					background-image url(../images/fotos/2festival.jpg)
				&.festival1
					background-image url(../images/fotos/1festival.jpg)
			.dados
				border-top 2px solid #00529B
				background-color #fff
				display block
				height 284px
				padding 10px
				width 100%
				div
					color #545454
					display block
					font-size 15px
					margin 0 0 10px
					position relative
					text-transform uppercase
					&:before
						color #ccc
						display block
						font-family icomoon
						left 0
						position absolute
						top 50%
						transform translateY(-50%)
					& > span
						border-left 1px solid #DEE1E3
						display block
						margin 0 0 0 50px
						padding 10px 0 10px 10px
						span
							display block
							line-height normal
							margin 0
							text-transform capitalize
					&.data:before
						content '\e900'
						font-size 25px
						height 25px
						left 13px
						width 23px
					&.comp:before
						content '\e904'
						font-size 22px
						height 22px
						left 9px
						width 30px
					&.local:before
						content '\e903'
						font-size 25px
						height 25px
						left 16px
						width 17px
					&.provas:before
						content '\e905'
						font-size 27px
						height 27px
						left 13px
						width 23px
	.btnNavegacao
		display block
		text-align center
		.btnPrev
		.btnNext
			border 2px solid #00529b
			color #005299
			cursor pointer
			display inline-block
			font-size 18px
			height 35px
			line-height 32px
			margin 0 10px
			text-align center
			vertical-align top
			width 130px
			&:hover
				background-color #00529b
				color #fff
#youtube
	iframe
		display block
		height 303px
		width 100%
	.button
		margin 30px auto 0
#contato
	form
		display block
		margin 0 auto
		width 100%
		#retornoHTML
			display block
			font-family 'ralewaybold'
			font-size 18px
			margin 0 0 20px
		input
			border 2px solid #fff
			color #666
			display block
			font-size 16px
			height 55px
			margin 0 0 20px
			padding 0 15px
			text-transform uppercase
			width 100%
			&:focus
				border-color #00509B
		textarea
			border 2px solid #fff
			color #666
			display inline-block
			font-size 16px
			height 206px
			margin 0 0 15px
			padding 15px
			text-transform uppercase
			vertical-align top
			width 100%
			&:focus
				border-color #00509B
		button
			background-color #f5f6f7
			&:hover
				background-color #00529B

footer
	background-color #222429
	.insta
	.face
		display inline-block
		vertical-align top
		width 100%
		h2
			color #fff
			font-size 40px
			margin 0 0 10px
			text-align left
			text-transform capitalize
	.insta
		h2
			padding 0 0 0 4px
		#instafeed
			a
				display inline-block
				height 102px
				vertical-align top
				margin 5px
				width 102px
				img
					display block
					height 100%
					width 100%
				.info
					display none
	.copy
		background-color #151619
		color #999
		font-size 16px
		height 60px
		line-height 60px
		text-align center















